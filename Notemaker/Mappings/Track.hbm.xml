<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="JayDev.MediaScribe" assembly="MediaScribe">

  <class name="Track" table="Tracks">

    <!-- A 32 hex character is our surrogate key. It's automatically
            generated by NHibernate with the UUID pattern. -->
    <id name="ID">
      <column name="TrackID" sql-type="integer" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="FilePath" />
    <!--<property name="ParentCourseID" column="CourseID" insert="false"/>-->
    <many-to-one name="ParentCourse" class="Course" column="CourseID" cascade="none" update="false" />
    <property name="Length" />
    <property name="IsVideo" />
    <property name="Title" />
    <property name="AspectRatio" />
    <property name="OrderNumber" />
    <property name="FileSize" />
  </class>

</hibernate-mapping>