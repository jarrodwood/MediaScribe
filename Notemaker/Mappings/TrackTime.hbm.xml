<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="JayDev.Notemaker" assembly="Notemaker">

  <class name="TrackTime" table="TrackTimes">

    <!-- A 32 hex character is our surrogate key. It's automatically
            generated by NHibernate with the UUID pattern. -->
    <id name="ID">
      <column name="TrackTimeID" sql-type="integer" not-null="true"/>
      <generator class="native" />
    </id>

    <property name="Time" />
    <many-to-one name="Track" fetch="join" class="Track" column="TrackID" cascade="none" />
  </class>

</hibernate-mapping>