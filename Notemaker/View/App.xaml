<Application  x:Class="JayDev.Notemaker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Resources scoped at the Application level should be defined here. -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="MyHuluStyle.xaml"/>
                <!--<ResourceDictionary Source="ExpressionDark.xaml"/>-->
                <ResourceDictionary Source="/Themes/JarrodDatagrid.xaml"/>
                <ResourceDictionary Source="/Themes/JarrodSlider.xaml"/>
                <ResourceDictionary Source="/Themes/JarrodGlassButton.xaml"/>
                <ResourceDictionary Source="/Themes/JarrodToggleButton.xaml"/>
                <ResourceDictionary Source="/Themes/JarrodBlueButton.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
