<Application  x:Class="JayDev.MediaScribe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Resources scoped at the Application level should be defined here. -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/JarrodDatagrid.xaml"/>
                <ResourceDictionary Source="/Styles/JarrodSlider.xaml"/>
                <ResourceDictionary Source="/Styles/JarrodGlassButton.xaml"/>
                <ResourceDictionary Source="/Styles/JarrodToggleButton.xaml"/>
                <ResourceDictionary Source="/Styles/JarrodBlueButton.xaml"/>
                <ResourceDictionary Source="/Styles/JarrodPlusMinusButtons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
